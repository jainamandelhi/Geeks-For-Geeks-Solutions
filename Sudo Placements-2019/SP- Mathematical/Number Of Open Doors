#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair
using namespace std;
int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        ll ans=0;
        for(ll i=0;i<=sqrt(n);i++)
        {
            if(i*i<=n)
                ans++;
        }
        cout<<ans-1<<endl;
    }
}
